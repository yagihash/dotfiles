[user]
  name = Yu YAGIHASHI
  email = yagihashoooooo@gmail.com
[core]
  pager = less -cmR
  excludesfile = /Users/yagihash/.gitignore_global
  editor = vim
[color]
  ui = auto
[push]
  defualt = current
[branch]
  autosetuprebase = always
[merge]
  ff = false
	tool = vimdiff
[pull]
  rebase = true
[diff]
  colorMoved = zebra
	tool = vimdiff
[alias]
  s = status
  l = log --oneline
  d = difftool
  p = pull
  graph = log --graph --date=short --decorate=short --pretty=format:'%Cgreen%h %Creset%cd %Cblue%cn %Cred%d %Creset%s'
[url "git@github.com:"]
  pushInsteadOf = git://github.com/
  pushInsteadOf = https://github.com/
[url "git@github.com:"]
  insteadOf = git://github.com/
  insteadOf = https://github.com/
[ghq]
	root = ~/ghq
  root = ~/go/src
[difftool]
	prompt = false
[mergetool]
	prompt = false
